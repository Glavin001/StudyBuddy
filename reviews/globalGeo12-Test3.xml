			<WORD>greenhouse</WORD>			<VALUE>0.5</VALUE>
		</KEY>
		<KEY>
			<WORD>photosynthesis</WORD> <WORD>respiration</WORD> <WORD>Albedo</WORD>			<VALUE>0.5</VALUE>
		</KEY>
	</ANSWER>
</QUESTION>
